{
  "extends": "../.dumi/tmp/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "../",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "ireact-material": ["components/index.tsx"],
      "ireact-material/es/*": ["components/*"],
      "ireact-material/theme/*": [".dumi/theme/*"]
    }
  },
  "include": ["./**/*", "../site/theme/template/Content"]
}
